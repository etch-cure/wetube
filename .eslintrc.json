{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "quotes" : ["error", "single"],
        "indent" : ["error", 4],
        "semi": ["error", "never"]
    }
}
